<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.grabitandgo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GrabItAndGo">
        <activity android:name=".CategoriesActivity"></activity>
        <activity android:name=".SettingsActivity" />
        <activity android:name=".OrdersActivity" />
        <activity android:name=".RewardsActivity" />
        <activity android:name=".ConnectActivity" />
        <activity android:name=".cardDetailsActivity"
            android:parentActivityName=".TermsConditionsActivity"/>
        <activity android:name=".TermsConditionsActivity"
            android:parentActivityName=".LoginActivity"/>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>